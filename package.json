{
  "name": "web-nusszopf",
  "version": "2.1.2",
  "description": "nusszopf monorepo",
  "repository": "git@github.com:nusszopf/web-nusszopf.git",
  "author": "schwamic <schwarz.michael@posteo.net>",
  "license": "GNU General Public License v3.0",
  "private": true,
  "workspaces": [
    "projects/*"
  ],
  "scripts": {
    "dev:webapp": "yarn --cwd projects/webapp dev",
    "dev:login": "yarn --cwd projects/auth-login dev",
    "dev:pw": "yarn --cwd projects/auth-password dev",
    "storybook": "yarn --cwd projects/ui-library start-storybook -p 6006"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{html,js,json,md,mdx,mjs,yml}": "prettier --write",
    "*.css": "stylelint --fix"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e",
      "pre-commit": "lint-staged"
    }
  },
  "engines": {
    "node": "12.x",
    "yarn": "1.22.x"
  },
  "dependencies": {
    "@reach/combobox": "0.11.2",
    "@reach/dialog": "0.11.2",
    "@reach/menu-button": "0.11.2",
    "@reach/tabs": "0.11.2",
    "auth0-js": "9.14.0",
    "classnames": "2.2.6",
    "formik": "2.2.5",
    "lodash": "4.17.20",
    "lucide-react": "0.1.2-beta.5",
    "next": "10.0.1",
    "next-seo": "4.17.0",
    "next-transpile-modules": "4.1.0",
    "prop-types": "15.7.2",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-feather": "2.0.8",
    "react-masonry-css": "1.0.14",
    "reakit": "1.3.0",
    "slate": "0.59.0",
    "slate-history": "0.59.0",
    "slate-react": "0.59.0",
    "smoothscroll-polyfill": "0.4.4",
    "typeface-barlow": "1.1.13",
    "yup": "0.29.3"
  },
  "devDependencies": {
    "@babel/core": "7.12.9",
    "@babel/register": "7.12.1",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@next/bundle-analyzer": "10.0.1",
    "autoprefixer": "10.0.2",
    "babel-eslint": "10.1.0",
    "babel-plugin-inline-react-svg": "1.1.2",
    "cross-env": "7.0.2",
    "eslint": "7.14.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "4.3.0",
    "lint-staged": "10.5.2",
    "postcss": "8.1.10",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-preset-env": "6.7.0",
    "prettier": "2.2.0",
    "stylelint": "13.8.0",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-recommended": "3.0.0",
    "tailwindcss": "2.0.1",
    "vercel": "20.1.4"
  }
}
