{
  "name": "web-nusszopf",
  "version": "4.3.0",
  "description": "nusszopf monorepo",
  "repository": "git@github.com:nusszopf/web-nusszopf.git",
  "author": "schwamic <schwarz.michael@posteo.net>",
  "license": "GNU General Public License v3.0",
  "private": true,
  "workspaces": [
    "projects/*"
  ],
  "scripts": {
    "dev:auth-login": "yarn --cwd projects/auth-login dev",
    "dev:auth-password": "yarn --cwd projects/auth-password dev",
    "dev:e2e": "yarn --cwd projects/e2e open",
    "dev:ui-library": "yarn --cwd projects/ui-library start-storybook -p 6006",
    "dev:webapp": "yarn --cwd projects/webapp dev",
    "lint": "eslint ./projects/*"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{html,js,json,md,mdx,mjs,yml}": "prettier --write",
    "*.css": "stylelint --fix"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e",
      "pre-commit": "lint-staged"
    }
  },
  "engines": {
    "node": "12.x",
    "yarn": "1.22.x"
  },
  "dependencies": {
    "@reach/combobox": "0.15.1",
    "@reach/dialog": "0.15.0",
    "@reach/tabs": "0.15.0",
    "auth0-js": "9.16.0",
    "classnames": "2.3.1",
    "compressorjs": "1.0.7",
    "fontsource-barlow": "4.0.0",
    "formik": "2.2.6",
    "framer-motion": "4.1.11",
    "lodash": "4.17.21",
    "lucide-react": "0.15.10",
    "next": "10.0.7",
    "next-seo": "4.19.0",
    "next-transpile-modules": "6.3.0",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-alice-carousel": "2.5.1",
    "react-dom": "17.0.2",
    "react-easy-crop": "3.3.3",
    "react-feather": "2.0.9",
    "react-masonry-css": "1.0.16",
    "react-merge-refs": "1.1.0",
    "reakit": "1.3.8",
    "slate": "0.62.0",
    "slate-history": "0.62.0",
    "slate-react": "0.62.0",
    "smoothscroll-polyfill": "0.4.4",
    "yup": "0.32.9"
  },
  "devDependencies": {
    "@babel/core": "7.14.0",
    "@babel/register": "7.13.16",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@next/bundle-analyzer": "10.2.0",
    "autoprefixer": "10.2.5",
    "babel-eslint": "10.1.0",
    "babel-plugin-inline-react-svg": "2.0.1",
    "cross-env": "7.0.3",
    "eslint": "7.25.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-cypress": "2.11.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.23.2",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "4.3.7",
    "lint-staged": "10.5.4",
    "postcss": "8.2.13",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-import": "14.0.1",
    "postcss-nesting": "7.0.1",
    "postcss-preset-env": "6.7.0",
    "prettier": "2.2.1",
    "stylelint": "13.13.0",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-recommended": "5.0.0",
    "tailwindcss": "2.1.2",
    "vercel": "20.1.4"
  }
}
